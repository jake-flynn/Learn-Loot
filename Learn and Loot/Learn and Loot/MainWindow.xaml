<Window x:Class="Learn_and_Loot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="768" Width="1028"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        WindowStyle="None"
        AllowsTransparency="False"
        ResizeMode="NoResize"
        >
    <Grid>
        <TextBox x:Name="tbx_title" HorizontalAlignment="Center" Height="54" IsReadOnly="True" Margin="392,14,407,0" TextWrapping="Wrap"  Text="Learn and Loot" VerticalAlignment="Top" Width="229" FontFamily="Segoe Print"/>
        <Button x:Name ="btn_Login" Content="Login" HorizontalAlignment="Center" Margin="937,10,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="tbx_mainContent" Height="100" Margin="100,88,100,0" TextWrapping="Wrap" Text="Main Content" VerticalAlignment="Top"/>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Content="Frame" Margin="10,210,10,10" />
        <Canvas HorizontalAlignment="Center" Height="520" Margin="10,231,0,0" VerticalAlignment="Top" Width="1002" />

    </Grid>
</Window>
